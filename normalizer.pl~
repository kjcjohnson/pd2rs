open IN_FILE, '<clean_data.csv';
open OUT_FILE, '>norm_data.csv';

@lines = <IN_FILE>;

$team1;
$team2;
$date;

foreach(@lines){
    if(/(.*),(.*),([0-9]+)/){
	$team1 = $1;
	$team2 = $2;
	$date = $3;
	if(&lengthen($team1)){
	    $team1 = &lenghten($team1);
	}
	if(&lengthen($team2)){
	    $team2 = &lenghten($team2);
	}
	print OUT_FILE ("$team1,$team2,$date");

    }
}

